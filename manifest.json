{
  "name": "Chromium ad-block",
  "description": "Chromium ad-blocker is an invisible ad-blocker created for Chromium-based browser",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "script.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "data/*.json"
  ]
}